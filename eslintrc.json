{
  "extends": ["standard", "semistandard-react"],
  "rules": {
    "semi": [
      2,
      "always"
    ],
    "no-extra-semi": 2,
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "ignore"
      }
    ]
  }
}
